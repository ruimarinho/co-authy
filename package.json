{
  "name": "authy-client",
  "version": "1.0.0",
  "description": "An Authy client",
  "license": "MIT",
  "homepage": "https://www.authy.com",
  "author": {
    "email": "rui.marinho@seegno.com",
    "name": "Rui Marinho",
    "url": "http://seegno.com"
  },
  "bugs": "https://github.com/seegno/co-authy/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/seegno/co-authy.git"
  },
  "main": "./dist/index.js",
  "keywords": [
    "2 factor",
    "2fa",
    "2step",
    "auth",
    "authentication",
    "authy",
    "client",
    "co",
    "two factor",
    "two step",
    "two-factor"
  ],
  "babel": {
    "stage": 0
  },
  "options": {
    "mocha": "--compilers js:babel/register --require should test"
  },
  "scripts": {
    "build": "rm -rf dist/* && babel src/ --out-dir dist/",
    "changelog": "github_changelog_generator --no-issues",
    "cover": "babel-node node_modules/.bin/isparta cover --report html node_modules/.bin/_mocha -- $npm_package_options_mocha",
    "coveralls": "babel-node node_modules/.bin/isparta cover --report html node_modules/.bin/_mocha --report lcovonly -- $npm_package_options_mocha && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint src test",
    "prepublish": "npm test && npm run build",
    "test": "mocha $npm_package_options_mocha"
  },
  "dependencies": {
    "bluebird": "^2.9.33",
    "creditcard": "^0.1.2",
    "debugnyan": "0.0.1",
    "google-libphonenumber": "^1.0.7",
    "lodash": "^3.9.3",
    "request": "^2.51.0",
    "require-dir": "^0.3.0",
    "validator.js": "^1.2.1",
    "validator.js-asserts": "^1.0.0",
    "world-countries": "^1.5.0"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-eslint": "^4.1.3",
    "coveralls": "^2.11.2",
    "eslint": "^1.5.1",
    "eslint-plugin-babel": "^2.1.1",
    "isparta": "^3.0.3",
    "mocha": "^2.0.1",
    "nock": "^2.7.0",
    "should": "^7.0.1",
    "sinon": "^1.15.4"
  },
  "engines": {
    "node": ">= 0.10",
    "iojs": ">= 1"
  }
}
